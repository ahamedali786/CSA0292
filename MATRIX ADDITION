#include<stdio.h>
int main()
{
    int i,j;
    int a[2][3] = {1,2,3,4,5,6};
    int b[2][3] = {9,8,7,6,5,4};
    int c[2][3] = {0,0,0,0,0,0};
    
    for(i=0;i<2;i++){
    for(j=0;j<3;j++){
            c[i][j] = a[i][j]+b[i][j];
        }
    }
    for(i=0;i<2;i++){
    for(j=0;j<3;j++){
        printf("%d\t",c[i][j]);
        }
        printf("\n");
    }
}

OUTPUT  :

10	10	10	
10	10	10	
